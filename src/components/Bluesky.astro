---
import agent from "../utils/bluesky";
import {BlueskyPost} from '@astro-community/astro-embed-bluesky'
export const prerender = false;
const response = await fetch(`${import.meta.env.DEPLOY_PRIME_URL || process.env.DEPLOY_PRIME_URL}/api/bluesky/latestPost`)
console.log('my prime url', import.meta.env.DEPLOY_PRIME_URL)
if (!response.ok) {
    throw new Error(`Response status: ${response.status}`);
}

const post = await response.json();
---

<BlueskyPost id={post.uri} />
